{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "systemName"            : "Forte",
    "footer"                : "Documentation generated by JSDoc",
    "copyright"             :  "{string}",
    "navType"               : "{vertical|inline}",
    "theme"                 : "united",
    "linenums"              : true,
    "collapseSymbols"       : true,
    "inverseNav"            : false,
    "outputSourceFiles"     : false,
    "outputSourcePath"      : true,
    "dateFormat"            : "{string}",
    "highlightTutorialCode" : true,
    "syntaxTheme"           : "default"
  }
}